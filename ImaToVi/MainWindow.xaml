                            <TextBlock x:Name="Name44" Grid.Column="1" FontSize="11"/>
                            <TextBlock x:Name="Count44" Grid.Column="1" Grid.Row="1" FontSize="11"/>
                            <TextBlock x:Name="Type44" Grid.Column="1" Grid.Row="2" FontSize="11"/>
                            <TextBlock x:Name="Size44" Grid.Column="1" Grid.Row="3" FontSize="11"/>
                            <TextBox Grid.ColumnSpan="2" Grid.RowSpan="4" Background="Transparent" BorderBrush="#00ABADB3" IsReadOnly="True" />
                        </Grid>
                    </Border>
                    <Button Content="X" HorizontalAlignment="Left" Margin="66,15,0,0" VerticalAlignment="Top" 
                    Width="16" Height="16" FontSize="10" Background="White" BorderBrush="#FF595959" />
                </Grid>
                <!--#endregion-->
            </Grid>
        </Border>
        <StackPanel Width="280">
            <Border Height="200" BorderThickness="3"  Margin="10,20,10,0" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}">
                <Image />
            </Border>
            <TextBlock Margin="0,20,0,0" Text="[timestamp]           ↓スタート位置" HorizontalAlignment="Center" FontSize="16"/>
            <Border Margin="10,3,10,0" BorderThickness="3" BorderBrush="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}">
                <Grid Height="100" >
                    <Grid.Resources>
                        <Style TargetType="{x:Type GridSplitter}">
                            <Setter Property="Background" Value="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}"/>
                            <Setter Property="IsEnabled" Value="False"/>
                            <Setter Property="HorizontalAlignment" Value="Stretch"/>
                        </Style>
                    </Grid.Resources>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="1*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="2*"/>
                    </Grid.ColumnDefinitions>
                    <GridSplitter Grid.Column="1" Grid.RowSpan="5" Width="3"/>
                    <GridSplitter Grid.Row="1" Grid.ColumnSpan="3" Height="3"/>
                    <GridSplitter Grid.Row="3" Grid.ColumnSpan="3" Height="3"/>
                    <RadioButton  Grid.Column="0" Grid.Row="0" Content="None" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,20,0" FontSize="14" Checked="None_Checked"/>
                    <RadioButton  Grid.Row="2" Content="deg" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,20,0" FontSize="14" Checked="Deg_Checked"/>
                    <RadioButton  Grid.Column="0" Grid.Row="4" Content="msec" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,20,0" FontSize="14" Checked="Msec_Checked"/>
                    <TextBox x:Name="radDeg" Grid.Column="2" Grid.Row="2" TextWrapping="Wrap" Text="0" HorizontalAlignment="Center" VerticalAlignment="Center" Width="100"/>
                    <TextBox x:Name="radMsec" Grid.Column="2" Grid.Row="4" TextWrapping="Wrap" Text="0" HorizontalAlignment="Center" VerticalAlignment="Center" Width="100"/>
                </Grid>
            </Border>
            <TextBlock TextWrapping="Wrap" Text="TimeStampSize" HorizontalAlignment="Center" FontSize="16" Margin="0,10,0,0"/>
            <Slider TickPlacement="TopLeft" Width="200" Foreground="Black" Margin="10" Maximum="60"/>
            <Grid Height="40" Margin="10,0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="83*"/>
                    <ColumnDefinition Width="135*"/>
                    <ColumnDefinition Width="42*"/>
                </Grid.ColumnDefinitions>
                <TextBlock Text="SaveDirectory" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" />
                <TextBox x:Name="SaveDirectory" Text="" Grid.Column="1" Margin="10" />
                <Button Content="..." Grid.Column="2" Margin="0,5" Click="SelectSaveFolder"/>

            </Grid>
            <StackPanel Height="77" Orientation="Horizontal">
                <Button Content="Convert" Width="74" Margin="40,20,30,20" Click="StartConvert"/>
                <Button Content="Exit" Width="75" Margin="30,20,0,20" Click="eClose"/>
            </StackPanel>
        </StackPanel>

    </StackPanel>

</Window>
